{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npx -y kill-port 5000 && nodemon --watch src --ext ts,js --exec \"cross-env DOTENV_DEBUG=false NODE_OPTIONS=--no-deprecation tsx src/server.ts\"",
    "clean": "del-cli \"dist\" \"src/**/*.js\" \"src/**/*.js.map\" \"src/**/*.d.ts\" \"src/**/*.d.ts.map\"",
    "prebuild": "npm run clean",
    "build": "tsc -p tsconfig.json",
    "prestart": "npm run build",
    "start": "npx -y kill-port 5000 && node dist/server.js",
    "db-check": "cross-env DOTENV_DEBUG=false NODE_OPTIONS=--no-deprecation tsx src/scripts/db-check.ts",
    "seed:admin": "cross-env DOTENV_DEBUG=false NODE_OPTIONS=--no-deprecation tsx src/scripts/seed-admin.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "ts-node": {
    "transpileOnly": true,
    "preferTsExts": true,
    "esm": true
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.3",
    "msnodesqlv8": "^5.1.3",
    "mssql": "^12.2.0",
    "nodemailer": "^7.0.12",
    "open": "^11.0.0",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.0.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tedious": "^19.2.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/hpp": "^0.2.7",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.7",
    "@types/nodemailer": "^7.0.5",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "cross-env": "^10.1.0",
    "del-cli": "^7.0.0",
    "nodemon": "^3.1.11",
    "rimraf": "^6.1.2",
    "ts-node": "^1.7.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
